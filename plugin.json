{
	"id": "nodebb-plugin-expiretopic",
	"url": "https://github.com/NodeBB/nodebb-plugin-expiretopic",
	"library": "./library.js",
	"hooks": [
		{ "hook": "static:app.load", "method": "init" },
		{ "hook": "filter:topic.thread_tools", "method": "addTopicThreadTools"},
		{ "hook": "filter:privileges.topics.get", "method": "getTopicPrivileges"}
	],
	"staticDirs": {
		"static": "./static"
	},
	"scss": [
		"static/scss/expiretopic.scss"
	],
	"scripts": [
		"static/lib/main.js"
	],
	"acpScripts": [
		"static/lib/acp-main.js"
	],
	"modules": {
		"../client/topic/expireTopic.js": "./static/modules/expireTopic.js"
	},
	"templates": "static/templates",
	"languages": "languages"
}